[merge]
    conflictstyle = diff3
[diff]
    algorithm = histogram
    # tool = difftastic
    context = 10
	submodule = log
[rerere]
    enabled = true
[merge]
    keepbackup = false
[rebase]
    autosquash = true
    autostash = true
[pull]
    rebase = true
[push]
    autoSetupRemote = true
[log]
    date = iso
[commit]
    verbose = true
[transfer]
	fsckobjects = true
[fetch]
	fsckobjects = true
[receive]
	fsckobjects = true
[status]
	submoduleSummary = true
[submodule]
	recurse = true
[branch]
	sort = -committerdate
